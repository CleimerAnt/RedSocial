@using RedSocial.Core.Application.Dtos.Account
@using RedSocial.Core.Application.Helpers;
@model RedSocial.Core.Application.Viewmodel.AccounsViewModel.UserPostViewModel;
@inject IHttpContextAccessor httpContext;
@{

    Layout = null;
    var user = httpContext.HttpContext.Session.get<AuthenticationResponse>
         ("User");
}

<section>
    <div class="container">
        <div class="row">
            <div class="col-6 mt-4">
                <img class="logo__img"
                     src="~/img/image_processing20191202-14263-ydyw15.png"
                     alt="" />
            </div>

            <div class="col-6">

                <h1 class="text-white mt-5">You do not have access to access this section </h1>
                <a asp-controller="User" asp-action="Index" asp-route-userId="@user.Id" class="btn btn-primary float-end">
                    log in
                </a>
            </div>
        </div>
    </div>
</section>